  "launcherClasses": {
    "15t-reusable": {
      "name": "15t Reusable First-Stage",
      "description": "Large reusable launcher optimized for LEO, 15t payload class with first-stage recovery",
      "payloadRangeLEO": [5000, 15000],
      "reusable": true,
      "addressability": {
        "Total market": {
          "percentage": 75,
          "rationale": "Weighted average across segments; excludes small-sat dedicated and some specialized missions"
        },
        "LEO constellations (comms + EO/IoT)": {
          "percentage": 90,
          "rationale": "Ideal fit: high-cadence bulk deployment, 1-5t per batch, LEO-optimized. Reusability economics maximize value. Only misses dedicated small-sat launches."
        },
        "Government civil (science + institutional)": {
          "percentage": 60,
          "rationale": "Good fit for medium science payloads (2-10t). Some missions too small (CubeSat deployers) or require specialized vehicles. Flagship missions may exceed capacity."
        },
        "Defense / national security": {
          "percentage": 70,
          "rationale": "Growing proliferated LEO constellations (SDA) are ideal. Some NRO/classified payloads require dedicated/specialized launchers. Heavy GEO reconnaissance may need larger vehicle."
        },
        "GEO comsat (single large satellites)": {
          "percentage": 40,
          "rationale": "Can serve market in expendable mode but less competitive vs heavy-lift. Modern GEO sats 4-7t fit capacity but economics favor dedicated GTO launchers."
        },
        "Human spaceflight + station cargo/logistics": {
          "percentage": 80,
          "rationale": "Excellent fit for crew/cargo to LEO stations. High-value, medium-cadence missions benefit from reusability. Some specialized crew vehicles may have dedicated launchers."
        },
        "Lunar / cislunar / exploration logistics": {
          "percentage": 50,
          "rationale": "Good for cislunar cargo in expendable mode or with refueling. Deep space missions often need heavier lift or specialized trajectories. Growing segment as Artemis scales."
        },
        "Other (tech demos, rideshare misc)": {
          "percentage": 30,
          "rationale": "Much of this segment is small-launcher territory (dedicated rideshare, university CubeSats). Only larger tech demos and institutional misc fit the 15t class."
        }
      }
    }
  },
